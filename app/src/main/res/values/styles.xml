<resources>

    <!--<style name="AppTheme" parent="Theme.AppCompat.Light">-->
        <!--&lt;!&ndash; Customize your theme here. &ndash;&gt;-->
    <!--</style>-->


    <style name="floatlabelededittext">
        <item name="android:textColor">#FF0000</item>
    </style>

    <declare-styleable name="ParallaxView">
        <attr name="speed" format="dimension" />
        <attr name="src" format="reference" />
    </declare-styleable>

    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@android:color/white</item>
    </style>

    <style name="WFrameLayout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="mwparent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <declare-styleable name="TextViewPlus">
        <attr name="customFont" format="string"/>
    </declare-styleable>


    <style name="WLayout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/riple</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">@dimen/four_lower</item>
        <item name="android:textStyle">italic</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:drawablePadding">@dimen/mf_10</item>
        <item name="android:gravity">left|center</item>

    </style>

    <style name="Theme.AppCompat.Light.NoActionBar.FullScreen" parent="@style/Theme.AppCompat.Light">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <style name="btn">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textSize">@dimen/twl_lower</item>
        <item name="android:textColor">@android:color/white</item>

    </style>
    <style name="tst">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:textSize">@dimen/twl_lower</item>
        <item name="android:textStyle">bold</item>
        <item name="android:padding">5dp</item>
        <item name="android:gravity">left|center</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@color/secondary_text</item>
    </style>

    <style name="view_ins">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:background">@color/primary</item>
    </style>

    <style name="spinner_style">
        <item name="android:textColor">@color/gold</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:popupBackground">@color/eoB</item>
    </style>
    <style name="edit_vari">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:layout_margin">10dp</item>
      ]
        <item name="android:drawablePadding">20dp</item>
        <item name="android:focusable">true</item>
        <item name="android:gravity">left|center</item>
        <item name="android:singleLine">true</item>
        <item name="android:hint">@string/remw</item>
        <item name="android:textColorHint">@color/eoB</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="chos_vari">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:background">@color/myTextPrimaryColor</item>
        <item name="android:gravity">left|center</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/text_secondary</item>
    </style>
    <style name="btm_vari">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:background">@color/myTextPrimaryColor</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:weightSum">2</item>

    </style>
    <style name="scong_page">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:gravity">left|center</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/brown</item>

    </style>


    <style name="set">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginRight">1dp</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:background">@color/myTextPrimaryColor</item>
        <item name="android:textSize">18sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/text_secondary</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:scrollHorizontally">true</item>

    </style>


    <style name="OverlayDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <!--android:fontFamily="sans-serif"           // roboto regular-->
    <!--android:fontFamily="sans-serif-light"     // roboto light-->
    <!--android:fontFamily="sans-serif-condensed" // roboto condensed-->
    <!--android:fontFamily="sans-serif-thin"      // roboto thin (android 4.2)-->
    <!--android:fontFamily="sans-serif-medium"    // roboto medium (android 5.0)-->


</resources>
